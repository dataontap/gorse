entrypoint = "main.py"
modules = ["nodejs-20", "postgresql-16", "python-3.11", "python3"]

[nix]
channel = "stable-24_05"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["sh", "-c", "python main.py"]
deploymentTarget = "cloudrun"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Run"
author = 36270138
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -c \"import socket; s = socket.socket(); s.bind(('0.0.0.0', 0)); print(f'Available port: {s.getsockname()[1]}'); s.close()\" && python main.py"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Flask Server"
author = "agent"

[workflows.workflow.metadata]
